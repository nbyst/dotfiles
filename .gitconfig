[user]
	name = nobuyasu.tomura
[credential]
	helper = cache --timeout=3600
[alias]
	rhh = reset --hard HEAD
	rld = log origin/master..master
	c = checkout
	cb = checkout -b
	b = branch
	bd = branch -d
	bdf = branch -D
	s = status 
	d = diff
	l = log
	rc = rm --cached
	ca = commit -a
	co = commit 
	hp = push -u origin master
	mm = merge master 
	m = merge
